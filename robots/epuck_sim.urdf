<?xml version="1.0" ?>
<robot name="differential_robot">
  <link name="base">
    <visual>
      <geometry>
        <cylinder length="0.03" radius="0.0375"/>
      </geometry>
      <origin xyz="0 0 0.015"/>
      <material name="green">
        <color rgba="0 1 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.03" radius="0.0375"/>
      </geometry>
      <origin xyz="0 0 0.005"/>
    </collision>
    <inertial>
      <origin xyz="0 0 0.005"/>
      <mass value="0.3"/>
      <inertia ixx="1e-4" iyy="1e-4" izz="2e-4" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>
  <link name="left_wheel">
    <visual>
      <geometry>
        <cylinder length="0.008" radius="0.02"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.008" radius="0.02"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
    </collision>
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="0.02"/>
      <inertia ixx="2.11e-6" iyy="4.00e-6" izz="2.11e-6" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>
  <joint name="left_wheel_joint" type="continuous">
    <parent link="base"/>
    <child link="left_wheel"/>
    <origin xyz="0 0.034 0.010" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>
  <link name="right_wheel">
    <visual>
      <geometry>
        <cylinder length="0.008" radius="0.02"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.008" radius="0.02"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
    </collision>
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="0.02"/>
      <inertia ixx="2.11e-6" iyy="4.00e-6" izz="2.11e-6" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>
  <joint name="right_wheel_joint" type="continuous">
    <parent link="base"/>
    <child link="right_wheel"/>
    <origin xyz="0 -0.034 0.010" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>
<link name="caster">
  <visual>
    <origin xyz="-0.03 0 0.005"/>
    <geometry>
      <sphere radius="0.008"/>
    </geometry>
    <material name="gray">
      <color rgba="0.7 0.7 0.7 1"/>
    </material>
  </visual>
  <collision>
    <origin xyz="-0.03 0 0.005"/>
    <geometry>
      <sphere radius="0.008"/>
    </geometry>
  </collision>
  <inertial>
    <origin xyz="0 0 0"/>
    <mass value="0.03"/>
    <inertia ixx="1e-6" iyy="1e-6" izz="1e-6" ixy="0" ixz="0" iyz="0"/>
  </inertial>
</link>
<joint name="caster_joint" type="fixed">
  <parent link="base"/>
  <child link="caster"/>
  <origin xyz="0 0 0"/>
</joint>
</robot>